{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "always"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "cSpell.words": [],
  "jest.enable": true,
  "jest.runMode": {
    "runAllTestsOnStartup": true,
    "showInlineError": true,
    "coverage": true,
    "deferred": true,
    "type": "on-save",
    "testFileOnly": true
  },
  "jest.debugMode": true
}
